    label: 'mypod',
    inheritFrom: 'default',
    containers: [
        containerTemplate(
            name: 'angular-cli',
            image: 'teracy/angular-cli:1.5.0',
            ttyEnabled: true,
            command: 'cat'
        )
    ]
) {
    node('mypod') {
        def commitId
        stage ('Build source'){
            container ('angular-cli') {
                sh 'ls -l'
            }
        }
    }
}